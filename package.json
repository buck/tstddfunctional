{
  "name": "tstddstarter",
  "version": "1.0.0",
  "description": "TypeScript TDD Starter",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "swc src --out-dir dist/src",
    "build:ci": "tsc && npm run build",
    "build:watch": "swc src -w --out-dir dist/src",
    "runscript": "node --import tsx bin/lairrun.ts",
    "start:dev": "concurrently \"npm run build:watch\" \"nodemon --watch 'dist/**/*.js' -e js dist/src/app.js\"",
    "test": "jest",
    "watch": "jest --watchAll"
  },
  "author": "A. Lester Buck III <buck@compact.com>",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@swc/jest": "^0.2.29",
    "@swc/plugin-transform-imports": "^1.5.108",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.172",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "concurrently": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^29.7.0",
    "jest-audio-reporter": "^2.2.3",
    "jest-cli": "^29.7.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.2",
    "npm-check-updates": "^16.14.12",
    "onchange": "^7.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "*",
    "ts-node": "^10.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "engines": {
    "node": ">=20.6"
  }
}
